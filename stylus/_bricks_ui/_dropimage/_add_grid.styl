.GRID-image-column
    width 100%
    height 100%
    background-image unquote('-webkit-gradient(radial, center center, 0, center center, 350, from(#e8e8e8), to(#9d9e9f))') !important
    background-image unquote('-moz-radial-gradient(center 45deg, circle closest-corner, #a1a2a3 0%, #767879 400)') !important
    filter unquote('progid:DXImageTransform.Microsoft.gradient(GradientType=0, startColorstr=#e1e1e1, endColorstr=#767879)')
    background-image unquote('gradient(radial, center center, 0, center center, 400, from(#a1a2a3), to(#767879))')
    display none
    UI-DROPIMAGE-CONTENT()